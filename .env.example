# .env.example
# --- CORE APPLICATION SETTINGS ---
APP_NAME="Financial Land AI"
ENVIRONMENT="dev"
HOST="0.0.0.0"
PORT=8000

# --- API KEYS ---
# Get your Gemini API Key from Google AI Studio
GEMINI_API_KEY=""

# API key for structured financial data (e.g., exchange rate service token)
FINANCIAL_DATA_API_KEY=""

# --- RAG/VECTOR DATABASE ---
# URL for your Vector Database service (e.g., Chroma, Qdrant, Pinecone)
VECTOR_DB_URL="http://localhost:8080"

# --- CURRENCY/EXCHANGE API ---
# Base URL for the ExchangeRate API (static part of the URL). Example: https://v6.exchangerate-api.com/v6
EXCHANGE_RATE_BASE_URL="https://v6.exchangerate-api.com/v6"
# If your exchange provider needs a separate API key (path param or header), set it here. Otherwise, leave blank.
EXCHANGE_RATE_API_KEY=""

# --- REGISTRY API (optional) ---
# If you have a company registry endpoint, set it here for the registry_check tool
REGISTRY_API_URL=""

# LLM Provider configuration
# Choose one of: gemini, groq
LLM_PROVIDER="gemini"
# GROQ provider API Key and model
GROQ_API_KEY=""
GROQ_MODEL="groq-1.0"

